package java_basics;

public class ArrayTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int [] numberArray = new int[5];  // declare 
		numberArray[0] = 10;
		numberArray[1] = 20;
		numberArray[2] = 30;
		numberArray[3] = 40;
		numberArray[4] = 50;
		System.out.println(numberArray);
		
		
	}

}
