package java_basics;

public class ArrayTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int [] numberArray = new int[5];  // declare 
		numberArray[0] = 0;
		numberArray[1] = 1;
		numberArray[2] = 2;
		numberArray[3] = 3;
		numberArray[4] = 4;
		System.out.println(numberArray);
		
		
	}

}
