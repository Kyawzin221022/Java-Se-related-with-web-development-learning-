package oop;

public class Car {

	long id;
	String brand;
	String model;
	String color;
	double price;
	int speedHour;
	String engineType;
	boolean ev;
	
	void engineStart()
	{
		System.out.println(id + "Engine is starting");
	}
	void drive()
	{
		System.out.println("Car is driving");
	}
	void changeGear()
	{
		System.out.println("Gear is changed");
	}
	void breake()
	{
		System.out.println("Car is breaked");
	}
	
}
